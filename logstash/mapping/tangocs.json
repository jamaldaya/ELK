{
  "index_patterns": [
    "tangocs-*"
  ],
  "template": "tangocs",
  "version": 1,
  "mappings": {
    "properties": {
      "device": {
        "properties": {
          "name": {
            "type": "keyword"
          },
          "server": {
            "type": "keyword"
          },
          "instance": {
            "type": "keyword"
          }
        }
      },
      "beamline": {
        "properties": {
          "name": {
            "type": "keyword"
          }
        }
      },
      "timestamp_nanos": {
        "type": "date_nanos",
        "format": "strict_date_optional_time_nanos"
      },
      "passerelle": {
        "properties": {
          "source": {
            "type": "keyword"
          },
          "actor": {
            "type": "keyword"
          }
        }
      }
    }
  }
}
